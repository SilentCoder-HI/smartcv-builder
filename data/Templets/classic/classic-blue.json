{
    "templateId": "classic-blue",
    "templateName": "Classic Blue",
    "description": "A polished blue resume template with serif typography and structured sections",
    "templateType": "classic",
    "version": "1.0",
    "plan": "free",
    "demo": true,
    "styles": {
      "root": {
        "backgroundColor": "white",
        "padding": "48px",
        "fontFamily": "serif",
        "color": "#1e3a8a",
        "width": "100%",
        "minHeight": "297mm",
        "maxWidth": "210mm",
        "margin": "0 auto"
      },
      "header": {
        "wrapper": {
          "textAlign": "center",
          "marginBottom": "2.5rem",
          "paddingBottom": "1.5rem",
          "borderBottom": "1px solid #93c5fd"
        },
        "h1": {
          "fontSize": "2.25rem",
          "fontWeight": "bold",
          "marginBottom": "0.25rem"
        },
        "jobTitle": {
          "fontSize": "1.125rem",
          "fontStyle": "italic",
          "marginBottom": "1rem",
          "color": "#1d4ed8"
        },
        "contact": {
          "container": {
            "display": "flex",
            "justifyContent": "center",
            "gap": "1.5rem",
            "fontSize": "0.875rem",
            "color": "#1d4ed8"
          },
          "item": {
            "display": "flex",
            "alignItems": "center"
          },
          "icon": {
            "width": "1rem",
            "height": "1rem",
            "marginRight": "0.25rem"
          }
        }
      },
      "summary": {
        "sectionTitle": {
          "fontSize": "1.25rem",
          "fontWeight": 600,
          "marginBottom": "0.75rem",
          "borderBottom": "1px solid #93c5fd",
          "paddingBottom": "0.25rem"
        },
        "summaryText": {
          "color": "#1e3a8a",
          "lineHeight": 1.6,
          "fontSize": "0.875rem",
          "textAlign": "justify"
        }
      },
      "layout": {
        "twoColumn": {
          "display": "grid",
          "gridTemplateColumns": "repeat(auto-fit, minmax(300px, 1fr))",
          "gap": "3rem"
        },
        "leftColumn": {
          "display": "flex",
          "flexDirection": "column",
          "gap": "2.5rem"
        },
        "rightColumn": {
          "display": "flex",
          "flexDirection": "column",
          "gap": "2.5rem"
        }
      },
      "experience": {
        "h3": {
          "fontSize": "1rem",
          "fontWeight": "bold",
          "color": "#1e3a8a"
        },
        "h4": {
          "fontSize": "0.875rem",
          "fontStyle": "italic",
          "color": "#1d4ed8"
        },
        "dateText": {
          "fontSize": "0.75rem",
          "color": "#3b82f6"
        },
        "summaryText": {
          "fontSize": "0.875rem",
          "marginTop": "0.5rem",
          "color": "#1e3a8a",
          "lineHeight": 1.6
        },
        "sectionTitle": {
          "fontSize": "1.25rem",
          "fontWeight": 600,
          "marginBottom": "0.75rem",
          "borderBottom": "1px solid #93c5fd",
          "paddingBottom": "0.25rem"
        }
      },
      "education": {
        "h3": {
          "fontSize": "1rem",
          "fontWeight": "bold",
          "color": "#1e3a8a"
        },
        "h4": {
          "fontSize": "0.875rem",
          "color": "#1d4ed8"
        },
        "dateText": {
          "fontSize": "0.75rem",
          "color": "#3b82f6"
        },
        "gpaText": {
          "fontSize": "0.75rem",
          "color": "#3b82f6"
        },
        "sectionTitle": {
          "fontSize": "1.25rem",
          "fontWeight": 600,
          "marginBottom": "0.75rem",
          "borderBottom": "1px solid #93c5fd",
          "paddingBottom": "0.25rem"
        }
      },
      "skills": {
        "skillCategory": {
          "fontWeight": "600",
          "color": "#1e3a8a",
          "fontSize": "1rem",
          "marginBottom": "0.25rem"
        },
        "skillItem": {
          "backgroundColor": "#dbeafe",
          "fontSize": "0.875rem",
          "padding": "0.25rem 0.75rem",
          "borderRadius": "9999px",
          "color": "#1e3a8a"
        },
        "container": {
          "display": "flex",
          "flexWrap": "wrap",
          "gap": "0.5rem"
        },
        "sectionTitle": {
          "fontSize": "1.25rem",
          "fontWeight": 600,
          "marginBottom": "0.75rem",
          "borderBottom": "1px solid #93c5fd",
          "paddingBottom": "0.25rem"
        }
      },
      "certifications": {
        "sectionTitle": {
          "fontSize": "1.25rem",
          "fontWeight": 600,
          "marginBottom": "0.75rem",
          "borderBottom": "1px solid #93c5fd",
          "paddingBottom": "0.25rem"
        },
        "list": {
          "listStyle": "disc",
          "paddingLeft": "1rem",
          "fontSize": "0.875rem",
          "color": "#1e3a8a"
        }
      },
      "languages": {
        "sectionTitle": {
          "fontSize": "1.25rem",
          "fontWeight": 600,
          "marginBottom": "0.75rem",
          "borderBottom": "1px solid #93c5fd",
          "paddingBottom": "0.25rem"
        },
        "list": {
          "listStyle": "disc",
          "paddingLeft": "1rem",
          "fontSize": "0.875rem",
          "color": "#1e3a8a"
        }
      },
      "hobbies": {
        "sectionTitle": {
          "fontSize": "1.25rem",
          "fontWeight": 600,
          "marginBottom": "0.75rem",
          "borderBottom": "1px solid #93c5fd",
          "paddingBottom": "0.25rem"
        },
        "list": {
          "listStyle": "disc",
          "paddingLeft": "1rem",
          "fontSize": "0.875rem",
          "color": "#1e3a8a"
        }
      }
    },
    "structure": {
      "layout": "two-column",
      "sections": [
        {
          "id": "header",
          "title": "Header",
          "fields": ["fullName", "jobTitle", "email", "phone", "address"]
        },
        {
          "id": "summary",
          "title": "Professional Summary",
          "fields": ["summary"]
        },
        {
          "id": "experience",
          "title": "Experience",
          "fields": ["position", "company", "startDate", "endDate", "current", "description"]
        },
        {
          "id": "education",
          "title": "Education",
          "fields": ["degree", "field", "institution", "startDate", "endDate", "gpa"]
        },
        {
          "id": "skills",
          "title": "Skills",
          "fields": ["category", "items"]
        },
        {
          "id": "certifications",
          "title": "Certifications",
          "fields": ["certifications"]
        },
        {
          "id": "languages",
          "title": "Languages",
          "fields": ["language", "proficiency"]
        },
        {
          "id": "hobbies",
          "title": "Hobbies",
          "fields": ["hobbies"]
        }
      ]
    }
  }
  